using System;
using System.Collections.Generic;

class Solution {
    public int solution(int[] A) {
        int len = A.Length;
        //stack for finiding leader
        Stack<int> leader = new Stack<int>();
        
        //number of equi leaders
        int equi = 0;
        
        
        //first element added
        leader.Push(A[0]);
        
        //find leader
        for(int i = 0; i < len; i++)
        {
            if(leader.Count == 0)
            {
                leader.Pop(A[i]);
            }
            else if(leader.Peek == A[i])
            {
                leader.Pop;
            }
            else
            {
                leader.Push(A[i]);
            }
        }
    }
}
