using System;

class Solution {
    public int[] solution(string S, int[] P, int[] Q) {
        //find length of string
        int N = S.Length;
        //declare multidimensional array for changes
        int[,] genes = new int[4, N+1];
        //accumulator
        int[] accum = new int[4];
        
        //record changes along sequence
        for(int i = 0; i < N; i++)
        {
            if (S.Substring(i,1) == "A")
            {
                accum[0]++;
            }
            else if (S.Substring(i,1) == "C")
            {
                accum[1]++;
            }
            else if (S.Substring(i,1) == "G")
            {
                accum[2]++;
            }
            else if (S.Substring(i,1) == "T")
            {
                accum[3]++;
            }
            
            //put accumulated values into genes array
            genes[0,i+1] = accum[0];
            genes[1,i+1] = accum[1];
            genes[2,i+1] = accum[2];
            genes[3,i+1] = accum[3];
        }
        
        //length of queries
        int len = P.Length;
        int[] result = new int[len];
        
        //check of genes exist in slice
        for(int i = 0; i < len; i++)
        {
            if(genes[0,Q[i]]-genes[0,P[i]] > 0)
            {
                result[i] = 1;
            }
            else if(genes[1,Q[i]]-genes[1,P[i]] > 0)
            {
                result[i] = 2;
            }
            else if(genes[2,Q[i]]-genes[2,P[i]] > 0)
            {
                result[i] = 3;
            }
            else if(genes[3,Q[i]]-genes[3,P[i]] > 0)
            {
                result[i] = 4;
            }
        }
        
        //return accumulated values
        return result;
    }
}
