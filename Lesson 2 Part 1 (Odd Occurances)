using System;
using System.Linq;

class Solution {
    public int solution(int[] A) {
        //declare variables
        int original = 0;
        int last = 0;
        int len = A.Length;
        
        //find out the number of pairs
        int repeat = len;
        repeat++;
        repeat = repeat/2;
        
        //repeat the sequence for the number of pairs there are
        for (int i = repeat; i > 0; i--)
        {
            //declaring n and s
            int n = 0;
            int s = 0;
            //find and match numbers
            for (; n < len; n++)
            {
                if (A[n] != 0)
                {
                    original = A[n];
                    A[n] = 0;
                    break;
                }
            }
            
            if (A.Contains(original))
            {
                s = Array.FindIndex(A, n, item => item == original);
                A[s] = 0;
            }
            else
            {
                A[n] = original;
            }
        }
        
        //find last remaining non-zero digit and return it
        for (int i = 0; i < len; i++)
        {
            if (A[i] != 0)
            {
                last = A[i];
                break;
            }
        }
        return last;
    }
}
